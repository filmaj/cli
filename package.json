{
  "name": "@architect/cli",
  "version": "6.0.19",
  "description": "A global cli tool for quickly building serverless apps with Architect",
  "main": "index.js",
  "bin": {
    "arc": "index.js"
  },
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "test:unit": "tape test/*-test.js | tap-spec",
    "lint": "eslint . --fix",
    "coverage": "istanbul cover tape test/*-test.js",
    "build": "pkg index.js && npm run tidy",
    "tidy": "rm -rf dist && mkdir dist && mv index-linux dist/arc-linux && mv index-macos dist/arc && mv index-win.exe dist/arc.exe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architect/cli.git"
  },
  "bugs": {
    "url": "https://github.com/architect/cli/issues"
  },
  "homepage": "https://github.com/architect/cli#readme",
  "keywords": [
    "aws",
    "serverless",
    "architect",
    "arc",
    "lambda"
  ],
  "author": "Brian LeRoux <b@brian.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/data": "^2.1.0",
    "@architect/deploy": "^1.0.0",
    "@architect/env": "^1.0.0",
    "@architect/hydrate": "^1.2.1",
    "@architect/logs": "^1.0.0",
    "@architect/package": "^1.0.30",
    "@architect/repl": "^1.0.0",
    "@architect/sandbox": "^1.2.2",
    "@architect/utils": "^1.1.0",
    "aws-sdk": "^2.491.0",
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "istanbul": "^0.4.5",
    "pkg": "^4.4.0",
    "sinon": "^7.3.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  }
}
