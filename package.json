{
  "name": "@architect/cli",
  "version": "6.0.14",
  "description": "A global cli tool for quickly building serverless apps with Architect",
  "main": "index.js",
  "bin": {
    "arc": "index.js"
  },
  "scripts": {
    "test": "tape test/*-test.js | tap-spec",
    "lint": "eslint src --fix",
    "build": "pkg index.js && npm run tidy",
    "tidy": "rm -rf dist && mkdir dist && mv index-linux dist/arc-linux && mv index-macos dist/arc && mv index-win.exe dist/arc.exe"
  },
  "keywords": [
    "aws",
    "serverless",
    "architect",
    "arc",
    "lambda"
  ],
  "author": "Brian LeRoux <b@brian.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/data": "^2.1.0",
    "@architect/hydrate": "^1.0.1",
    "@architect/package": "^1.0.18",
    "@architect/parser": "^1.1.7",
    "@architect/sandbox": "^1.0.6",
    "@architect/utils": "^1.0.14",
    "aws-sdk": "^2.480.0",
    "chalk": "^2.4.2",
    "glob": "^7.1.4",
    "mime-types": "^2.1.24",
    "mkdirp": "^0.5.1",
    "path-sort": "^0.1.0",
    "run-parallel": "^1.1.9",
    "run-series": "^1.1.8",
    "run-waterfall": "^1.1.6",
    "sha": "^3.0.0",
    "strftime": "^0.10.0",
    "zip-dir": "^1.0.2",
    "zipit": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "pkg": "^4.4.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.2"
  }
}
